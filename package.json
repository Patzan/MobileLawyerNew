    "test": "ng test",
    "lint": "ng lint",
    "copy-google-services": "cp google-services.json android/app/google-services.json",
    "sync": "ionic build && npx cap sync android && npm run copy-google-services",
    "bdev": "ionic build && npx cap sync android && ionic capacitor run android",
    "bfdev": "ionic build --configuration=fdev && npx cap sync android",
    "brc": "ionic build --configuration=rc && npx cap sync android && ionic capacitor run android",
    "bstg": "ionic build --configuration=staging && npx cap sync android",
    "bpprod": "ionic build --configuration=preprod && npx cap sync android",
    "bprod": "ionic build --configuration=production && npx cap sync android",
    "emul": "ionic capacitor run android -l --external"
  },
